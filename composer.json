{
    "name": "jkwamlah/firestore-php",
    "description": "Firestore PHP Client",
    "keywords": [
        "php",
        "firestore",
        "firebase",
        "google",
        "cloud firestore",
        "firestore database"
    ],
    "homepage": "https://github.com/jkwamlah/firestore-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonadab Kwamlah",
            "email": "19jonadab@gmail.com",
            "homepage": "https://www.linkedin.com/in/jonadab-kwamlah-3a9692138",
            "role": "Software Engineer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "~7.0|~6.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "MrShan0\\PHPFirestore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MrShan0\\PHPFirestore\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
